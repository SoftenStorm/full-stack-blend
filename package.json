{
    "name": "full-stack-blend",
    "version": "0.1.0",
    "engines": {
        "node": "12.14.x"
    },
    "description": "Full-Stack Development Platform For Web Application",
    "repository": {
        "type": "git",
        "url": "https://github.com/jasukkas/full-stack-blend.git"
    },
    "author": "SoftenStorm.com",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-sass && npm run build-ts && npm run build-bundle && npm run lint && npm run copy-static-assets",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-bundle\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "babel src --out-dir dist --extensions '.ts,.tsx'",
        "watch-ts": "babel src --out-dir dist --extensions '.ts,.tsx' --watch",
        "build-bundle": "browserify dist/public/js/construction/editor/Editor.js -o dist/public/js/construction/Editor.bundle.js && browserify dist/public/js/construction/editor/Worker.js -o dist/public/js/construction/Worker.bundle.js && browserify dist/public/js/construction/area/Html.js -o dist/public/js/construction/Html.bundle.js && browserify dist/public/js/construction/distribution/Embed.js -o dist/public/js/Embed.bundle.js",
        "watch-bundle": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"watchify dist/public/js/construction/editor/Editor.js -o dist/public/js/construction/Editor.bundle.js\" \"watchify dist/public/js/construction/editor/Worker.js -o dist/public/js/construction/Worker.bundle.js\" \"watchify dist/public/js/construction/area/Html.js -o dist/public/js/construction/Html.bundle.js\" \"watchify dist/public/js/construction/distribution/Embed.js -o dist/public/js/Embed.bundle.js\"",
        "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css && node-sass src/public/css/construction/distribution/embed.scss -o dist/public/css/ && node-sass src/public/css/construction/area/area.scss -o dist/public/css/construction/ && node-sass src/public/css/construction/editor/editor.scss -o dist/public/css/construction/",
        "watch-sass": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"node-sass -w src/public/css/main.scss -o dist/public/css/main.css\" \"node-sass -w src/public/css/construction/distribution/embed.scss -o dist/public/css/\" \"node-sass -w src/public/css/construction/area/area.scss -o dist/public/css/construction/\" \"node-sass -w src/public/css/construction/editor/editor.scss -o dist/public/css/construction/\"",
        "lint": "eslint \"src/**/*.{js,ts}\" --quiet --fix",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-sass-area\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "dependencies": {
        "amd-module": "0.0.20151203",
        "assert": "^2.0.0",
        "async": "^3.1.0",
        "babel-polyfill": "^6.26.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-mongo": "^3.0.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-force-https": "^1.0.0",
        "express-session": "^1.16.2",
        "express-validator": "^6.2.0",
        "fbgraph": "^1.4.4",
        "lodash": "^4.17.15",
        "lusca": "^1.6.1",
        "mongoose": "^5.7.1",
        "nodemailer": "^6.3.0",
        "passport": "^0.4.0",
        "passport-facebook": "^3.0.0",
        "passport-github": "^1.1.0",
        "passport-local": "^1.0.0",
        "pug": "^2.0.4",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.4",
        "@babel/plugin-transform-react-jsx": "^7.8.3",
        "@babel/plugin-transform-regenerator": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@types/archiver": "^3.1.0",
        "@types/async": "^3.0.2",
        "@types/bcrypt-nodejs": "^0.0.30",
        "@types/bluebird": "^3.5.27",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.3",
        "@types/compression": "^1.0.1",
        "@types/concurrently": "^4.1.0",
        "@types/connect-mongo": "^3.0.0",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/express-flash": "0.0.1",
        "@types/express-session": "^1.15.14",
        "@types/jest": "^24.0.23",
        "@types/jquery": "^3.3.31",
        "@types/lodash": "^4.14.141",
        "@types/lusca": "^1.6.1",
        "@types/mongoose": "^5.5.18",
        "@types/node": "^12.7.8",
        "@types/node-sass": "^4.11.0",
        "@types/nodemailer": "^6.2.1",
        "@types/passport": "^1.0.1",
        "@types/passport-facebook": "^2.1.9",
        "@types/passport-local": "^1.0.33",
        "@types/pug": "^2.0.4",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/shelljs": "^0.8.5",
        "@types/supertest": "^2.0.8",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "archiver": "^4.0.1",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.3",
        "babel-plugin-react-auto-binding": "^1.0.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-pug-html": "^0.1.3",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-plugin-transform-react-pug": "^7.0.1",
        "babelify": "^10.0.0",
        "chai": "^4.2.0",
        "concurrently": "^5.2.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.4.0",
        "eslint-plugin-react": "^7.18.3",
        "jest": "^24.9.0",
        "node-sass": "^4.13.1",
        "nodemon": "^1.19.2",
        "shelljs": "^0.8.3",
        "sinon": "^9.0.2",
        "supertest": "^4.0.2",
        "ts-jest": "^24.3.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.7.5",
        "watchify": "^3.11.1"
    }
}
